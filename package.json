    "moment": "2.16.0",
    "rx": "4.1.0",
    "rx-angular": "1.1.3"
  },
  "xo": {
    "globals": [
      "angular",
      "app",
      "describe",
      "beforeEach",
      "inject",
      "it",
      "expect",
      "module",
      "require",
      "$"
    ],
    "rules": {
      "linebreak-style": [
        0
      ],
      "new-cap": 1,
      "no-invalid-this": 0,
      "strict": [
        2,
        "function"
      ],
      "brace-style": [
        2,
        "stroustrup",
        {
          "allowSingleLine": false
        }
      ]
    },
    "ignores": [
      ".tmp/**",
      ".git/**",
      "build/**"
    ]
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "gulp build&&gulp serve",
    "prod": "gulp build --production",
    "test": "gulp tests",
    "serve": "gulp serve",
    "doc": "gulp build-doc&&gulp serve-doc"
  },
