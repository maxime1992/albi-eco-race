{
  "name": "albi-eco-race",
  "version": "1.0.0",
  "description": "Albi-eco-race website",
  "main": "Gulpfile.js",
  "dependencies": {
    "angular": "1.5.8",
    "angular-animate": "1.5.8",
    "angular-loading-bar": "0.9.0",
    "angular-mocks": "1.5.8",
    "angular-sanitize": "1.5.8",
    "angular-timer": "1.3.3",
    "angular-translate": "2.13.0",
    "angular-translate-loader-static-files": "2.13.0",
